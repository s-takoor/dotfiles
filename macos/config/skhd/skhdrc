# LAUNCH APPLICATIONS
shift + cmd - return : open -n -a "Alacritty"
shift + cmd - a : open -n -a "Amethyst"
shift + cmd - b : open -n -a "Brave Browser"
shift + cmd - d : open ~/Documents
shift + cmd - e : emacsclient -c -n -a ''
shift + cmd - l : open -n -a Launchpad
shift + cmd - o : open ~/Documents/OrgFiles
shift + cmd - s : open -n -a "Slack"
shift + cmd - k : open -n -a "System Settings"
