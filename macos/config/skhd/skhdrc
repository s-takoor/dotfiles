# -- LAUNCH APPLICATIONS -- #
shift + cmd - return : alacritty
shift + cmd - b : open -na "Brave Browser"
shift + cmd - e : emacsclient -c -n -a ''
shift + cmd - l : open -na Launchpad
shift + cmd - d : open ~/Documents
shift + cmd - o : open ~/Documents/orgfiles
shift + cmd - s : open -na "Slack"
shift + cmd - k : open -na "System Settings"

# -- MPD CONTROL -- #
ctrl + alt - p : mpc toggle
ctrl + alt - n : mpc next
ctrl + alt - b : mpc prev
